    "cpus": 0.1,
    "mem": 50,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "nsqio/nsq:v0.3.7",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 4150,
                    "hostPort": 4150
