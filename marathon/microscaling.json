{
    "id": "microscaling",
    "instances": 1,
    "cpus": 0.1,
    "mem": 100,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "quay.io/rossf7/microscaling:test"
        }
    },
    "env": {
        "MSS_SCHEDULER": "MARATHON",
        "MSS_USER_ID": "__MSS_USER_ID__",
        "MSS_API_ADDRESS": "__MSS_API_ADDRESS__",
        "MSS_MARATHON_API": "__MSS_MARATHON_API__"
    }
}
