{
    "id": "microscaling",
    "instances": 1,
    "cpus": 0.1,
    "mem": 100,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "quay.io/rossf7/microscaling:test",
            "forcePullImage": true
        }
    },
    "env": {
        "MSS_SCHEDULER": "MARATHON",
        "MSS_USER_ID": "__MSS_USER_ID__",
        "MSS_API_ADDRESS": "__MSS_API_ADDRESS__",
        "MSS_MARATHON_API": "http://__MSS_MARATHON_API__",
        "AZURE_STORAGE_ACCOUNT_NAME": "__AZURE_STORAGE_ACCOUNT_NAME__",
        "AZURE_STORAGE_ACCOUNT_KEY": "__AZURE_STORAGE_ACCOUNT_KEY__",
        "MSS_KU": "0.03",
        "MSS_TU": "0.7"
    }
}
